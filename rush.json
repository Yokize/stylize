{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.39.1",
  "pnpmVersion": "5.17.2",
  "projectFolderMaxDepth": 3,
  "suppressNodeLtsWarning": true,
  "ensureConsistentVersions": true,
  "repository": {
    "url": "https://github.com/Yokize/stylize.git"
  },
  "projects": [
    {
      "packageName": "@stylize/sass-tool",
      "projectFolder": "common/tools/sass",
      "shouldPublish": false
    },
    {
      "packageName": "@stylize/sass-func",
      "projectFolder": "package/sass-func",
      "shouldPublish": true
    },
    {
      "packageName": "@stylize/sass-mixin",
      "projectFolder": "package/sass-mixin",
      "shouldPublish": true
    },
    {
      "packageName": "@stylize/sass-triangle",
      "projectFolder": "package/sass-triangle",
      "shouldPublish": true
    }
  ]
}
